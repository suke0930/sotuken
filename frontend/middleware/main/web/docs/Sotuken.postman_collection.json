							"// Extract session cookie from Set-Cookie (preferred), headers, body, or request Cookie header and save to env var \"sessionCookie\"",
							"(function () {",
							"  const ENV_VAR = 'sessionCookie';",
							"  const ASSUMED_NAME = 'frontdriver-session';",
							"  const saveMode = 'overwrite';",
							"  const redactLen = 6; // not used here, just for reference",
							"",
							"  const res = pm.response;",
							"  const headers = res.headers;",
							"  const body = res.text ? res.text() : (res.body || '');",
							"",
							"  // Helpers",
							"  function setEnv(val) {",
							"    if (!val) return false;",
							"    pm.environment.set(ENV_VAR, val);",
							"    return true;",
							"  }",
							"  function parseCookiesFromSetCookie() {",
							"    const setCookies = headers.filter(h => h.key.toLowerCase() === 'set-cookie').map(h => h.value);",
							"    if (!setCookies.length) return null;",
							"    // Prefer cookie whose name looks like frontdriver-session or similar",
							"    let target = null;",
							"    for (const sc of setCookies) {",
							"      // capture name=value at the start",
							"      const m = sc.match(/^\\s*([^=;\\s]+)\\s*=\\s*([^;\\n]+)/);",
							"      if (!m) continue;",
							"      const name = m[1];",
							"      const value = m[2];",
							"      if (!target || name.toLowerCase().includes('session') || name.toLowerCase().includes('frontdriver')) {",
							"        target = `${name}=${value}`;",
							"        if (name === ASSUMED_NAME) break;",
							"      }",
							"    }",
							"    return target;",
							"  }",
							"  function parseFromHeaders() {",
							"    // Try all headers as a fallback",
							"    const cookieHeader = headers.get ? headers.get('Set-Cookie') : null;",
							"    if (cookieHeader) {",
							"      const m = cookieHeader.match(/^\\s*([^=;\\s]+)\\s*=\\s*([^;\\n]+)/);",
							"      if (m) return `${m[1]}=${m[2]}`;",
							"    }",
							"    return null;",
							"  }",
							"  function parseFromBody() {",
							"    if (!body) return null;",
							"    // Try explicit frontdriver-session first",
							"    let m = body.match(/(?:^|;|\\n)\\s*(frontdriver-session)=([^;\\n]+)/i);",
							"    if (m) return `${m[1]}=${m[2]}`;",
							"    // Generic name=value fallback with session-ish name",
							"    m = body.match(/(?:^|;|\\n)\\s*([^=;\\s]*session[^=;\\s]*)\\s*=\\s*([^;\\n]+)/i);",
							"    if (m) return `${m[1]}=${m[2]}`;",
							"    return null;",
							"  }",
							"  function parseFromRequestCookieHeader() {",
							"    const reqCookie = pm.request.headers.get('Cookie');",
							"    if (!reqCookie) return null;",
							"    // Try to find frontdriver-session first",
							"    let m = reqCookie.match(/(?:^|;|\\n)\\s*(frontdriver-session)=([^;\\n]+)/i);",
							"    if (m) return `${m[1]}=${m[2]}`;",
							"    // else pick first cookie that looks like a session",
							"    m = reqCookie.match(/(?:^|;|\\n)\\s*([^=;\\s]*session[^=;\\s]*)\\s*=\\s*([^;\\n]+)/i);",
							"    if (m) return `${m[1]}=${m[2]}`;",
							"    return null;",
							"  }",
							"",
							"  let cookieKV = null;",
							"  // Strategy priority: set-cookie, responseHeaders, responseBody, requestHeaderCookie",
							"  cookieKV = parseCookiesFromSetCookie();",
							"  if (!cookieKV) cookieKV = parseFromHeaders();",
							"  if (!cookieKV) cookieKV = parseFromBody();",
							"  if (!cookieKV) cookieKV = parseFromRequestCookieHeader();",
							"",
							"  if (cookieKV && setEnv(cookieKV)) {",
							"    pm.test('Session cookie captured', function () {",
